{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/contests(?:/([^/]+?))$",
      "dest": "/contests/$1",
      "check": true
    },
    {
      "src": "^(?:/(.*))$",
      "dest": "/index.html",
      "check": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "crons": []
}
